{
    "name": "Quick Apply",
    "description": "Extension to make applying to jobs quicker",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
        "default_popup": "index.html",
        "default_title": "Open the popup"
    },
    "icons": {
        "16": "link-solid.png",
        "48": "link-solid.png",
        "128": "link-solid.png"
    },
    "permissions": ["storage", "tabs"]
}
